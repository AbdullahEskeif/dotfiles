port 465
from abdullah.m.eskef@gmail.com
user abdullah.m.eskef@gmail.com
passwordeval "pass abdullah.m.eskef@gmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/Aboud/.local/state/msmtp/msmtp.log
tls_starttls off
